{
    "workbench.colorTheme": "Black Ocean",
    "editor.wordWrap": "bounded",
    "editor.wordWrapColumn": 200,
    "window.menuBarVisibility": "toggle",
    "editor.fontFamily": "monospace",
    "liveServer.settings.donotShowInfoMsg": true,
    "liveServer.settings.donotVerifyTags": true,
    "C_Cpp.clang_format_fallbackStyle": "GNU",
    "workbench.colorCustomizations": {
        "[Black Ocean]": {
            "editor.background": "#0C0E10"
        },
        "editorIndentGuide.background": "#232525ce"
    },
    "C_Cpp.updateChannel": "Insiders",
    "editor.minimap.showSlider": "always",
    "problems.showCurrentInStatus": true,
    "gnuGlobal.completion": "Disabled",
    "gnuGlobal.debugMode": "Enabled",
    "C_Cpp.intelliSenseEngineFallback": "Enabled",
    "C_Cpp.default.intelliSenseMode": "gcc-x64",
    "C_Cpp.enhancedColorization": "Disabled",
    "workbench.tree.renderIndentGuides": "always",
    "editor.renderLineHighlight": "all",
    "editor.tabSize": 2,
    "workbench.iconTheme": "material-icon-theme",
    "vikings-icon-theme.showWelcomeMessage": false,
    "material-icon-theme.folders.color": "#26a69a",
    "material-icon-theme.folders.theme": "classic",
    "window.zoomLevel": 0,
    "editor.multiCursorModifier": "ctrlCmd",
    "verilog.linting.linter": "modelsim",
    "editor.smoothScrolling": true,
    "HDL.linting.linter": "modelsim",
    "HDL.linting.modelsim.runAtFileLocation": true,
    "HDL.linting.iverilog.runAtFileLocation": true,
    "verilog.linting.iverilog.runAtFileLocation": true,
    "verilog.linting.modelsim.runAtFileLocation": true,
    "verilog.linting.verilator.runAtFileLocation": true,
}